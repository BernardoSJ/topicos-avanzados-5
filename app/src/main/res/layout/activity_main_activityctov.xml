<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityctov"
    tools:layout_editor_absoluteY="81dp">


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="140dp"
        android:text="Barra 1:"
        android:textSize="18sp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView12"
        android:layout_marginTop="183dp"
        android:layout_alignLeft="@+id/textView12" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView12"
        android:layout_marginStart="113dp"
        android:text="Barra 2:"
        android:textSize="18sp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="113dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView13"
        android:layout_alignTop="@+id/spinner1"
        android:layout_alignLeft="@+id/textView13" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView12"
        android:layout_marginEnd="115dp"
        android:text="Barra 3:"
        android:textSize="18sp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="115dp" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView14"
        android:layout_alignTop="@+id/spinner1"
        android:layout_alignLeft="@+id/textView14" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView12"
        android:layout_marginEnd="22dp"
        android:text="Barra 4:"
        android:textSize="18sp"
        android:layout_marginRight="22dp"
        android:layout_alignParentRight="true" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView15"
        android:layout_alignTop="@+id/spinner1"
        android:layout_alignLeft="@+id/textView15" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="45dp"
        android:text="Resistencias"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView12"
        android:layout_marginBottom="182dp"
        android:background="@android:color/darker_gray"
        android:layout_alignLeft="@+id/textView12" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView17"
        android:layout_marginEnd="39dp"
        android:background="@android:color/darker_gray"
        android:layout_alignParentRight="true"
        android:layout_marginRight="39dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="190dp"
        android:layout_height="95dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="149dp"
        android:layout_marginStart="3dp"
        android:layout_toEndOf="@+id/textView12"
        android:background="@android:color/holo_blue_dark"
        android:layout_marginLeft="3dp"
        android:layout_toRightOf="@+id/textView12" />

    <TextView
        android:id="@+id/b4"
        android:layout_width="17dp"
        android:layout_height="93dp"
        android:layout_alignEnd="@+id/textView14"
        android:layout_alignTop="@+id/textView19"
        android:background="@android:color/background_dark"
        android:layout_alignRight="@+id/textView14" />

    <TextView
        android:id="@+id/b3"
        android:layout_width="17dp"
        android:layout_height="93dp"
        android:layout_alignStart="@+id/textView14"
        android:layout_alignTop="@+id/textView19"
        android:background="@android:color/background_dark"
        android:layout_alignLeft="@+id/textView14" />

    <TextView
        android:id="@+id/b2"
        android:layout_width="17dp"
        android:layout_height="93dp"
        android:layout_alignEnd="@+id/spinner2"
        android:layout_alignTop="@+id/textView19"
        android:background="@android:color/background_dark"
        android:layout_alignRight="@+id/spinner2" />

    <TextView
        android:id="@+id/b1"
        android:layout_width="17dp"
        android:layout_height="93dp"
        android:layout_alignStart="@+id/textView13"
        android:layout_alignTop="@+id/textView19"
        android:background="@android:color/background_dark"
        android:layout_alignLeft="@+id/textView13" />

    <Button
        android:id="@+id/calcular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView12"
        android:layout_marginBottom="12dp"
        android:text="Calcular"
        android:layout_alignLeft="@+id/textView12" />

    <Button
        android:id="@+id/reiniciar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/calcular"
        android:layout_centerHorizontal="true"
        android:text="Reiniciar" />

    <Button
        android:id="@+id/regresar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView18"
        android:layout_alignTop="@+id/calcular"
        android:text="regresar"
        android:layout_alignRight="@+id/textView18" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/calcular"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="91dp"
        android:text="Resultados:"
        android:textSize="18sp"
        android:layout_alignRight="@+id/calcular" />

    <TextView
        android:id="@+id/resultados"
        android:layout_width="202dp"
        android:layout_height="65dp"
        android:layout_above="@+id/textView25"
        android:layout_marginBottom="-46dp"
        android:layout_marginEnd="-144dp"
        android:layout_toStartOf="@+id/textView14"
        android:textSize="18sp"
        android:layout_marginRight="-144dp"
        android:layout_toLeftOf="@+id/textView14" />

</RelativeLayout>